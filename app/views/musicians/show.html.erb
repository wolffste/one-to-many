<%= @musician.username %>

<% if !current_user %>
  <%= form_for(@favorite) do |f| %>
  <%= f.label :user %>
  <%= f.text_field :user %>
  <%= f.text_field :musician %>
  <%= button_tag( :class => "btn star") do %>
    <i class="far fa-star"></i>
  <% end %>
  <% end %>
<% else %>
  <%= form_for(@favorite) do |f| %>
    <%= f.label :user %>
  <%= f.hidden_field :user_id, value: current_user.id %>
  <%= f.hidden_field :musician_id, value: @musician.id %>
   <%= button_tag( :class => "btn star") do %>
    <i class="far fa-star"></i>
  <% end %>
  <% end %>
<% end %>








