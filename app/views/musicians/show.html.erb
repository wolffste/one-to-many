<%= @musician.username %>

<% if !current_user %>
  <%= form_for(@favorite) do |f| %>
  <%= f.label :user %>
  <%= f.text_field :user %>
  <%= f.text_field :musician %>
  <%= f.submit "Bookmark", class: "btn btn-danger pic large", id:"button" %>
  <% end %>
<% else %>
  <%= form_for(@favorite) do |f| %>
    <%= f.label :user %>
  <%= f.hidden_field :user_id, value: current_user.id %>
  <%= f.hidden_field :musician_id, value: @musician.id %>
  <%= f.submit "Bookmark", class: "btn btn-danger pic large", id:"button" %>
  <% end %>
<% end %>


<div class="container">
  <div class="row">
    <div class="col-xs-6 col-md-4 portfolio-item">
      <div class="card-otm">
        <img class="img-fluid" src="https://images.unsplash.com/photo-1559519529-0936e4058364?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=634&q=80" alt="">
      </div>
    </div>
    <div class="col-xs-12 col-md-8">
      <div class="card-otm h-100">
        <ul style="list-style-type: none;">
          <li>name: <strong><%= @musician.username %></strong></li>
          <li>e-mail: <strong><%= @musician.email %></strong></li>
          <li>city: <strong><%= @musician.city %></strong></li>
          <li>date of birth: <strong><%= @musician.birthday %></strong></li>
          <li>description: <strong><%= @musician.description %></strong></li>
        </ul>
      </div>
    </div>
  </div>
  <div class="row mt-4">
    <div class="col-12">
      <div class="card-otm">
        <h1>dsfggd</h1>
      </div>
    </div>
  </div>
</div>






